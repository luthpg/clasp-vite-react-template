{
  "name": "clasp-vite-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "push": "clasp push",
    "push:watch": "clasp push -w",
    "deploy": "dotenv cross-var \"clasp deploy -i $GAS_DEPLOYMENT_ID\"",
    "open:script": "clasp open",
    "open:web": "clasp open --webapp",
    "test": "jest --config ./jest.config.json --silent=true --coverage",
    "test:console": "jest --config ./jest.config.json",
    "lint": "eslint --ext .tsx,.ts src/ && eslint --ext .ts server/",
    "format": "prettier --write \"**/*.+(js|json|ts|tsx)\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/google-apps-script": "^1.0.59",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-testing-library": "^5.10.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.2.0",
    "vite-plugin-singlefile": "^0.13.5"
  }
}
